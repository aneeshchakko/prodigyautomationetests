{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "https://www.prodigygame.com/"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "login-link"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "login-username"
      },
      "text": "aneesh.chakko+poweruser@prodigygame.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "login-password"
      },
      "text": "test"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//form[@id='login-form']//button[.='Login']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//header[@id='main-header']//a[.=' My Students']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='manage-students']//span[.='Add Students']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='choices-options']//button[.='Create New Students']"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "class-create-first-1"
      },
      "text": "Alex"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "class-create-last-1"
      },
      "text": "Prodigy"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='class-create-entry']//button[.='Create Students']"
      }
    },
    {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='class-create-complete-actions']//button[.='Close']"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Alex Prodigy"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}